<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.referazi.dao.SkillDao">
    <resultMap id="SkillResultMap" type="com.referazi.models.Skill">
        <result column="profile_type" property="profileType"/>
        <result column="skill_name" property="skillName" />
    </resultMap>
    <select id="getSkills" resultType="String">
        select skill_name from skills where profile_type=#{profileType}
    </select>
</mapper>